<div >
    <input type="text" ng-model="searchText" ng-change="update()" style="width: 400px;"  my-enter="freeTextSearch()" />
  <div  class="autocomplete-result" ng-show="results.length > 0"  style='background:white;'>
    Suggested tags:
  </div>
  <div  class="autocomplete-result" ng-repeat="category in results"  style='background:white;'>
    <a ng-href="#/asset/results/byTag/and/{{category._id}}">
      {{category._id}}
    </a>
  </div>
</div>